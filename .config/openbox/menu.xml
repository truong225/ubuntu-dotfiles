				<execute>command</execute>
			</action>
		</item>
	</menu>
	<menu execute="/usr/libexec/openbox-xdg-menu applications" id="applications-menu" label="Applications"/>
	<menu execute="/usr/libexec/openbox-xdg-menu preferences" id="preferences-menu" label="Preferences"/>
	<menu execute="/usr/libexec/openbox-xdg-menu system-settings" id="administration-menu" label="Administration"/>
	<menu execute="/usr/libexec/openbox-xdg-menu /etc/xdg/openbox/terminals" id="terminals-menu" label="Terminals"/>
	<menu id="root-menu" label="Openbox 3">
		<separator label="[ Openbox ]"/>
		<item label="Apps">
			<action name="Execute">
				<execute>rofi -combi-modi window,drun,ssh,run -show combi -modi combi,run -display-drun Start</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-71489" label="Accessories">
			<separator label="Accessories"/>
			<menu id="root-menu-71489-57125" label="File Manager">
				<separator label="File Manager"/>
				<item label="Nautilus">
					<action name="Execute">
						<execute>nautilus</execute>
					</action>
				</item>
			</menu>
			<menu id="root-menu-71489-340" label=" Text Editor">
				<separator label="Text Editor"/>
				<item label="Gedit">
					<action name="Execute">
						<execute>gedit</execute>
					</action>
				</item>
				<item label="Sublime Text">
					<action name="Execute">
						<execute>sh ~/.bin/sublime_text</execute>
					</action>
				</item>
			</menu>
		</menu>

		<menu id="inet" label="Internet">
			<separator label="Internet"/>
			<menu id="inet-51719" label="Chat">
				<separator label="Chat"/>
				<item label="Skype">
					<action name="Execute">
						<execute>skypeforlinux</execute>
					</action>
				</item>
			</menu>
			<menu id="inet-11613" label="Browser">
				<separator label="Browser"/>
				<item label="Mozilla Firefox">
					<action name="Execute">
						<execute>firefox</execute>
					</action>
				</item>
			</menu>
		</menu>
		<menu id="office" label="Office">
			<separator label="Office"/>
			<item label="Libre">
				<action name="Execute">
					<execute>libreoffice</execute>
				</action>
			</item>
		</menu>
		<menu id="multimedia" label=" Multimedia">
			<separator label=" Multimedia"/>
			<menu id="multimedia-843818" label=" Picture Viewer">
				<separator label=" Picture Viewer"/>
				<item label="Feh">
					<action name="Execute">
						<execute>feh</execute>
					</action>
				</item>
			</menu>
