			<separator label="Tools"/>
			<menu id="root-menu-161838-221457" label="Panel">
				<separator label="Panel"/>
				<item label="Tint2">
					<action name="Execute">
						<execute>tint2</execute>
					</action>
				</item>
				<item label="Tint2 Config">
					<action name="Execute">
						<execute>tint2conf</execute>
					</action>
				</item>
			</menu>
			<menu id="root-menu-161838-131059" label="Terminal">
				<separator label="Terminal"/>
				<item label="Urxvt">
					<action name="Execute">
						<execute>urxvt</execute>
					</action>
				</item>
				<item label="Urxvt Transparent">
					<action name="Execute">
						<execute>urxvt --transparent</execute>
					</action>
				</item>
				<item label="Gnome-Terminal">
					<action name="Execute">
						<execute>gnome-terminal</execute>
					</action>
				</item>
