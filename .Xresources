Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter: lcddefault
Xft.autohint: true

URxvt.intensityStyles:  false
!URxvt.font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
!URxvt.font: -misc-fixed-bold-r-normal--15-140-75-75-c-90-iso8859-1
URxvt.font:  xft:Iosevka:bold:size=10
URxvt.boldFont:xft:Iosevka:bold:size=11

XTerm.font: xft:Iosevka:bold:size=11
UXTerm.font: xft:Iosevka:bold:size=11

!            xft:Kochi Gothic:antialias=false,\
!            xft:Baekmuk Gulim


URxvt.saveLines:        8192

URxvt*scrollTtyOutput:    true
URxvt*scrollWithBuffer:   true
URxvt*scrollTtyKeypress:  true
URxvt.scrollBar:          true
URxvt.scrollColor:	#efefef
!URxvt.scrollstyle:	rxvt
URxvt.scrollstyle:     plain
URxvt.thickness:	15

URxvt.troughColor:	#fefefe
URxvt.internalBorder:     30
URxvt.cursorBlink:        true
URxvt.cursorUnderline:    true
URxvt*skipBuiltinGlyphs:    true

URxvt.perl-ext-common: font-size
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show
URxvt.letterSpace: 1
!URxvt.geometry:	110x26
URxvt.geometry:	77x17

!URxvt*borderColor: #D9D7CB

*foreground:        #6D6168
*background:        #fefefe
*.foreground:       #6D6168
*.background:       #fefefe

emacs*foreground:   #6D6168
emacs*background:   #fefefe
URxvt*foreground:   #6D6168
XTerm*foreground:   #6D6168
UXTerm*foreground:  #6D6168
URxvt*background:   [100]#fcfdfd
XTerm*background:   #fefefe
UXTerm*background:  #fefefe
URxvt*cursorColor:  #6D6168
XTerm*cursorColor:  #6D6168
UXTerm*cursorColor: #6D6168
URxvt*borderColor:  [100]#fefefe

